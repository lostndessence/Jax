<?xml version='1.0' encoding='utf-8'?>
<widget id="com.jaxcohome.app" version="1.0.4" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Jaxcohome</name>
    <description>
        Explore Jaxcohome with ease from your mobile phone. 
    </description>
    <author email="rolando@ad-in.us" href="http://ad-in.us">
        Ad.In Advertising
    </author>
    <content src="index.html" />
    <access origin="*" />
	
	<plugin name="cordova-plugin-inappbrowser" spec="~1.7.1" />
	<plugin name="cordova-plugin-network-information" spec="~1.3.3" />
	
  
  <!-- Assign Icon-->
  <icon src="icon.png" /><!-- Default -->
  
  <platform name="ios">
	<icon src="icon-ios.png" width="180" height="180" />
	<splash src="Splash_Screen.png" platform="ios" width="320" height="480" />
  </platform>
  
  <platform name="android" >
	<splash src="Splash_Screen.png" platform="android" width="320" height="480" />
  </platform>
  
  <platform name="winphone" />
  
</widget>
