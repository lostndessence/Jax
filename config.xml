<?xml version='1.0' encoding='utf-8'?>
<widget id="com.jaxcohome.app" version="1.0.5" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Jaxcohome</name>
    <description>
        Explore Jaxcohome with ease from your mobile phone. 
    </description>
    <author email="rolando@ad-in.us" href="http://ad-in.us">
        Ad.In Advertising
    </author>
    <content src="index.html" />
	
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#ee6e73" />
    <preference name="StatusBarStyle" value="blacktranslucent" />
    <plugin name="cordova-plugin-statusbar" source="npm" spec="~2.1.3" />
    
	<plugin name="phonegap-plugin-push" source="npm" spec="~1.8.0">
        <variable name="SENDER_ID" value="XXXXXXX" />
    </plugin>
	
    <plugin name="cordova-plugin-console" source="npm" spec="~1.0.3" />
    <plugin name="cordova-plugin-dialogs" source="npm" spec="~1.2.1" />
    <plugin name="cordova-plugin-whitelist" source="npm" spec="~1.2.1" />	
	
	
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
	
	<plugin name="cordova-plugin-inappbrowser" spec="~1.7.1" />
	<plugin name="cordova-plugin-network-information" spec="~1.3.3" />
	
  
  <!-- Assign Icon-->
  <icon src="icon-ios.png" /><!-- Default -->
  
  <!--<platform name="ios">
	<icon src="icon-ios.png" platform="ios" width="180" height="180" />
	<splash src="Splash_Screen.png" platform="ios" width="320" height="480" />
  </platform>
  
  <platform name="android" >
	<splash src="Splash_Screen.png" platform="android" width="320" height="480" />
  </platform>
   
  <platform name="winphone" />
  <platform name="windows" /> -->
  
</widget>
